<apply template="/_layout">
	<project><article class="project">
		<h1><name>Project Name</name></h1>

		<div class="listing">
			<component><article>
				<apply template="_component" />

				<image><img src="/images/screenshots/${slug}/${filename}" width="${width}" height="${height}" alt="" /></image>
			</article></component>
		</div>
	</article></project>
</apply>